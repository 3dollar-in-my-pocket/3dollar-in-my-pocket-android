<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="item"
            type="com.zion830.threedollars.repository.model.v2.response.store.BossStoreFeedbackTypeResponse.BossStoreFeedbackTypeModel" />

    </data>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="6dp"
        android:background="@drawable/rect_white_stroke_gray"
        android:orientation="vertical">

        <CheckBox
            android:id="@+id/reviewCheckBox"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:button="@null"
            android:drawableStart="@drawable/selector_type_radio_review"
            android:drawablePadding="24dp"
            android:padding="14dp"
            android:textColor="@color/black"
            android:textSize="16sp"
            tools:text="음식이 맛있어요" />

    </androidx.appcompat.widget.LinearLayoutCompat>
</layout>